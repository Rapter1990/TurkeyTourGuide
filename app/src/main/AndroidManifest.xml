<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.turkeytourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:label="@string/city_list"
            android:name=".MainActivity" />
        <activity
            android:label="@string/attractive_places_list"
            android:name=".AttactivePlacesActivity" />
        <activity
            android:label="@string/attractive_place"
            android:name=".AttactivePlaceObject">
        </activity>
    </application>

</manifest>